msgstr "maelstrom"

#. [object], id=29
#: ./For_Power/chapter5/utils/cave.cfg:949
msgid "Archmage's Coat"
msgstr "Abrigo del Archimago"

#. [object], id=29
#: ./For_Power/chapter5/utils/cave.cfg:951
